    "@types/react-native": "0.60.23",
    "@types/react-test-renderer": "16.9.1",
    "babel-jest": "24.9.0",
    "jest": "24.9.0",
    "jetifier": "1.6.4",
    "metro-react-native-babel-preset": "0.57.0",
    "patch-package": "6.2.0",
    "react-test-renderer": "16.9.0",
    "typescript": "3.7.2"
  },
  "jest": {
    "preset": "react-native",
